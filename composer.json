{
  "name": "angujo/elolara",
  "description": "An all round Laravel model generator",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Barrack Angujo",
      "email": "angujomondi@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.1",
    "ext-json": "*"
  },
  "autoload": {
    "files": [
      "src/constants.php",
      "src/helpers.php"
    ],
    "psr-4": {
      "Angujo\\Elolara\\": "src/"
    }
  },
  "require-dev": {
  },
  "extra": {
    "laravel": {
      "providers": [
        "Angujo\\Elolara\\Laravel\\ElolaraServiceProvider"
      ],
      "aliases": {
        "Elolara": "Angujo\\Elolara\\Laravel\\Factory"
      }
    }
  }
}
