{
  "name": "angujo/elolara",
  "description": "An all round Laravel model generator",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Barrack Angujo",
      "email": "angujomondi@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.1"
  },
  "autoload": {
    "files": [
      "src/constants.php",
      "src/helpers.php"
    ],
    "psr-4": {
      "Angujo\\LaravelModel\\": "src/"
    }
  },
  "require-dev": {
  },
  "extra": {
    "laravel": {
      "providers": [
        "Angujo\\LaravelModel\\Laravel\\LaravelModelServiceProvider"
      ],
      "aliases": {
        "LaravelModel": "Angujo\\LaravelModel\\Laravel\\Factory"
      }
    }
  }
}
